{
  "name": "archinsight-ui",
  "private": true,
  "version": "1.0.0",
  "description": "React UI for Archinsight",
  "author": "",
  "license": "AGPL-3.0",
  "scripts": {
    "antlr": "rimraf build/insight-lang && mkdir -p build/insight-lang && antlr4ts ../insight-lang/src/main/antlr/InsightLexer.g4 ../insight-lang/src/main/antlr/InsightParser.g4 -Xexact-output-dir -o build/insight-lang",
    "build:dev": "webpack serve --env dev",
    "build:report": "webpack --env report",
    "build:prod": "webpack",
    "report": "webpack-bundle-analyzer --port 4200 ./dist/stats.json",
    "serve": "serve ./dist",
    "format": "prettier --write \"src/**/*.{ts,tsx,js}\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js}\" --fix"
  },
